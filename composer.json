{
    "name": "STP5940/appbase_slim",
    "description": "Appbase Slim Framework Application",
    "keywords": [
      "Appbase slim framework",
      "Slim MVC"
    ],
    "homepage": "https://github.com/STP5940/appbase_slim",
    "license": "MIT",
    "authors": [
        {
            "name": "Sitthipong Sengdonprai",
            "email": "sitthipong114@gmail.com",
            "homepage": "https://www.facebook.com/home.htmI"
        }
    ],
    "require": {
        "php": ">=5.6",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "larapack/dd": "1.1",
        "mpdf/mpdf": "8.0.0",
        "eftec/bladeone": "3.24",
        "illuminate/database": "^5.3",
        "vlucas/phpdotenv": "3.3.3",
        "slim/csrf": "0.8.3",
        "tuupola/slim-jwt-auth": "3.4.0",
        "bryanjhv/slim-session": "~3.0",
        "endroid/qr-code": "3.6.1",
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "app/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php start --port 8888",
        "test": "phpunit"
    }
}
